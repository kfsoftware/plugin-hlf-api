	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch Package",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "${workspaceFolder}/main.go",
			"args": [
				"serve",
				"--channel=demo",
				"--chaincode=basic",
				"--peers=127.0.0.1:7020,127.0.0.1:7116",
				"--tlscerts=./tls-ca.pem,./tls-ca.pem",
				"--mspid=Org1MSP",
				"--key=./key.pem",
				"--cert=./cert.pem"
			]
		}
	]
}